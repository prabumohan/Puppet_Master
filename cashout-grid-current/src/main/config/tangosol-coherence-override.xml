<?xml version='1.0'?>
<coherence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.oracle.com/coherence/coherence-operational-config"
	xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-operational-config coherence-operational-config.xsd">

	<cluster-config>
		<member-identity>
			<cluster-name system-property="tangosol.coherence.cluster">${cashout.grid.coherence.clusterName}</cluster-name>
		</member-identity>
		<multicast-listener>
			<join-timeout-milliseconds system-property="sbet.join.timeout.ms">6000</join-timeout-milliseconds>
		</multicast-listener>
		<packet-publisher>
			<packet-delivery>
				<timeout-milliseconds>120000</timeout-milliseconds>
			</packet-delivery>
		</packet-publisher>
		<incoming-message-handler>
			<packet-pool>
				<size>8MB</size>
			</packet-pool>
		</incoming-message-handler>
	</cluster-config>

	<logging-config>
		<destination>${cashout.grid.coherence.log.destination}</destination>
		<severity-level system-property="tangosol.coherence.log.level">${cashout.grid.coherence.log.level}</severity-level>
	</logging-config>
	
	<management-config>
      <managed-nodes system-property="tangosol.coherence.management">local-only</managed-nodes>
   </management-config>

</coherence>